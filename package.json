{
	"name": "tldraw-in-obsidian",
	"version": "1.26.2",
	"description": "Integrates Tldraw into Obsidian, allowing users to draw and edit content on a virtual whiteboard.",
	"main": "dist/production/main.js",
	"private": true,
	"scripts": {
		"clean": "rm -rf dist && rm -rf node_modules && yarn",
		"dev": "NODE_ENV=development node esbuild.config.mjs",
		"build": "NODE_ENV=production node esbuild.config.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "yarn run -T tsx ../../internal/scripts/lint.ts",
		"test": "yarn run -T vitest --passWithNoTests",
		"test-ci": "yarn run -T vitest run --passWithNoTests",
		"test-coverage": "yarn run -T vitest run --coverage --passWithNoTests"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"tldraw",
		"drawing",
		"whiteboard",
		"notes"
	],
	"author": "Sam Alhaqab",
	"contributors": [
		"Jonathan Mendez <dev@jon-dez.com> (https://github.com/sponsors/jon-dez)",
		"Sam Alhaqab"
	],
	"devDependencies": {
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "7.12.0",
		"@typescript-eslint/parser": "7.12.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.23.1",
		"obsidian": "^1.10.0",
		"tslib": "2.6.3",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"monkey-around": "^2.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tldraw": "workspace:*",
		"zustand": "^4.3.9"
	},
	"peerDependencies": {
		"react": "^18.2.0 || ^19.0.0",
		"react-dom": "^18.2.0 || ^19.0.0"
	}
}
